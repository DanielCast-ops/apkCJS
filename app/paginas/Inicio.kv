<Inicio>:
    name: 'Inicio'
    BoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: "Inicio"
            left_action_items: [["menu", lambda x: app.on_menu_click()]]
        Widget:
        MDRaisedButton:
            text: "Ir a configuraciones"
            pos_hint: {"center_x": 0.5}
            on_release: app.root.current = 'Configuraciones'
        MDRaisedButton:
            text: "Ir a inventario"
            pos_hint: {"center_x": 0.5}
            on_release: app.root.current = 'Invenrario'

